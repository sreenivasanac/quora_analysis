{
  "buildCommand": "cd visualization/visualization_frontend && npm run build",
  "outputDirectory": "visualization/visualization_frontend/build",
  "installCommand": "npm install --prefix visualization/visualization_frontend",
  "routes": [
    {
      "src": "/api/timestamps/all",
      "dest": "/api/timestamps_all.py"
    },
    {
      "src": "/api/timestamps",
      "dest": "/api/timestamps.py"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}