{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "visualization/visualization_frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd visualization/visualization_frontend && npm run build",
        "distDir": "visualization/visualization_frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/timestamps/all",
      "dest": "/api/timestamps_all.py"
    },
    {
      "src": "/api/timestamps",
      "dest": "/api/timestamps.py"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/visualization/visualization_frontend/build/$1"
    }
  ]
}